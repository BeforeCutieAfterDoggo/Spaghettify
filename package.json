{
  "publisher": "BCAD",
  "name": "spaghettify",
  "displayName": "Spaghettify",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "",
  "author": "BCAD <hello@bcad.one>",
  "license": "MIT",
  "homepage": "https://github.com/beforecutieafterdoggo/spaghettify#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/beforecutieafterdoggo/spaghettify.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "spaghettify.introduceBug",
        "title": "Spaghettify - Introduce Bug"
      },
      {
        "command": "spaghettify.obscureCode",
        "title": "Spaghettify - Obscure Code"
      },
      {
        "command": "spaghettify.setup",
        "title": "Spaghettify - Setup"
      }
    ]
  },
  "configuration": {
    "type": "object",
    "properties": {
      "spaghettify.apiKey": {
        "type": "string",
        "default": "",
        "description": "OpenAI API Key"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0"
  },
  "dependencies": {
    "openai": "^3.1.0"
  }
}
